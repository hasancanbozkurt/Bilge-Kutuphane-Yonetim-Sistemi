@using BilgeKutuphanesi.Models.Entity
@model List<TBLDuyurular>
    @foreach (var x in Model)
    {
<div class="post">
    <div class="user-block">
        <img class="img-circle img-bordered-sm" src="~/AdminLTE-3.0.4/dist/img/2.png" alt="user image">
        <span class="username">
            <p>@x.KATEGORI</p>
        </span>
        <span class="description">@x.TARIH.ToString()</span>
    </div>
    @x.ICERIK
</div>
    }

